scalar DateTime

type Query {
    # Retorna todas as consultas
    consultas: [Consulta!]!
}

enum Especialidade {
    CARDIOLOGIA
    PEDIATRIA
    ORTOPEDIA
    DERMATOLOGIA
}

type Consulta {
    id: ID!
    pacienteId: ID!
    medicoId: ID!
    especialidade: Especialidade!
    dataHoraConsulta: DateTime!
    observacoes: String
}
